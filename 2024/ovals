#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;
    int count = 0;
    while (n > 0) {
        int digit = n % 10;   // take last digit
        if (digit == 0 || digit == 6 || digit == 9) count += 1;
        else if (digit == 8) count += 2;
        n = n / 10;           // remove last digit
    }
    cout << count;
    return 0;
}
