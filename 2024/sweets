#include <iostream>
using namespace std;

int main() {
    long long A, B;
    cin >> A >> B;

    long long x = (2*A - B);
    long long y = (2*B - A);

    if (x % 3 != 0 || y % 3 != 0) {
        cout << -1;
        return 0;
    }

    x /= 3;
    y /= 3;

    if (x < 0 || y < 0) {
        cout << -1;
    } else {
        cout << x << " " << y;
    }

    return 0;
}
