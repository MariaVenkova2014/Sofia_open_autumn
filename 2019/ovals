#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;

    int cnt = 0;
    if (n == 0) cnt = 1; // специален случай, защото 0 има 1 овалче

    while (n > 0) {
        int d = n % 10;
        if (d == 0 || d == 6 || d == 9) cnt++;
        if (d == 8) cnt += 2;
        n /= 10;
    }

    cout << cnt << endl;
}
