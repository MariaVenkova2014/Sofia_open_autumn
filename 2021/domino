#include <iostream>
using namespace std;

int main() {
    int m;
    int top[4], bottom[4];
    cin >> m;
    for (int i = 0; i < 4; i++) {
        cin >> top[i] >> bottom[i];
    }

    // Първоначални суми
    int sumTop = 0, sumBottom = 0;
    for (int i = 0; i < 4; i++) {
        sumTop += top[i];
        sumBottom += bottom[i];
    }

    // Опитваме всяка подредица
    for (int start = 0; start <= 4 - m; start++) {
        int newTop = sumTop, newBottom = sumBottom;
        for (int j = 0; j < m; j++) {
            int idx = start + j;
            newTop = newTop - top[idx] + bottom[idx];
            newBottom = newBottom - bottom[idx] + top[idx];
        }
        if (newTop == newBottom) {
            cout << start + 1 << "\n";
            return 0;
        }
    }

    cout << "N/A\n";
    return 0;
}
