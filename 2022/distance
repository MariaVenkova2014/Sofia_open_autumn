#include <iostream>
using namespace std;

int main() {
    int K1, M1, C1;
    int K2, M2, C2;
    int K3, M3, C3;

    cin >> K1 >> M1 >> C1;
    cin >> K2 >> M2 >> C2;
    cin >> K3 >> M3 >> C3;

    // преобразуваме всичко в сантиметри
    int distance1 = K1 * 100000 + M1 * 100 + C1; 
    int distance2 = K2 * 100000 + M2 * 100 + C2; 
    int distance3 = K3 * 100000 + M3 * 100 + C3; 

    // реалните разстояния от града
    int d1 = distance1;
    int d2 = distance1 + distance2;
    int d3 = distance1 + distance3;

    // намираме най-голямото
    int maxDistance = d1;
    if (d2 > maxDistance) maxDistance = d2;
    if (d3 > maxDistance) maxDistance = d3;

    // връщаме обратно в км, м и см
    int km = maxDistance / 100000;
    int m  = (maxDistance % 100000) / 100;
    int cm = maxDistance % 100;

    cout << km << " " << m << " " << cm << endl;
}
