#include <iostream>
#include <iomanip>
using namespace std;

int main() {
    int D, M, Y;
    cin >> D >> M >> Y;

    // Дни в месеците (за невисокосна година)
    int daysInMonth[13] = {0,31,28,31,30,31,30,31,31,30,31,30,31};

    // Проверка за високосна година
    bool leap = (Y % 400 == 0) || (Y % 4 == 0 && Y % 100 != 0);
    if (leap) daysInMonth[2] = 29;

    // специален случай: 4 октомври 1582 -> 15 октомври 1582
    if (D == 4 && M == 10 && Y == 1582) {
        cout << "15 10 1582" << endl;
        return 0;
    }

    // увеличаваме деня
    D++;

    // ако сме минали броя дни в месеца
    if (D > daysInMonth[M]) {
        D = 1;
        M++;
        if (M > 12) {
            M = 1;
            Y++;
        }
    }

    // печат с водещи нули: ден 2, месец 2, година 4
    cout << setw(2) << setfill('0') << D << " "
         << setw(2) << setfill('0') << M << " "
         << setw(4) << setfill('0') << Y << endl;

    return 0;
}
