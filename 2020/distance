#include <iostream>
using namespace std;

int main() {
    int K1, M1, C1;
    int K2, M2, C2;
    int K3, M3, C3;
    cin >> K1 >> M1 >> C1 >> K2 >> M2 >> C2 >> K3 >> M3 >> C3;

    // Преобразуваме Жгмътц в сантиметри
    int D1 = K1 * 100000 + M1 * 100 + C1;
    // Аялайя = Жгмътц + разстоянието ѝ
    int D2 = D1 + K2 * 100000 + M2 * 100 + C2;
    // Шпльотц = Жгмътц + разстоянието му
    int D3 = D1 + K3 * 100000 + M3 * 100 + C3;

    int maxDist = max(D1, max(D2, D3));

    // Връщаме обратно в км, м, см
    int km = maxDist / 100000;
    maxDist %= 100000;
    int m = maxDist / 100;
    int cm = maxDist % 100;

    cout << km << " " << m << " " << cm << endl;
}
